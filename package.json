  "dependencies": {
    "p5": "^1.7.0",
    "solid-js": "^1.7.8"
  },
  "devDependencies": {
    "@types/p5": "^1.7.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-solid": "^2.7.0"
  }
}